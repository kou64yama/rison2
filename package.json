{
  "name": "rison2",
  "version": "0.2.3",
  "description": "Rison2 is a parser/stringifier of Rison",
  "type": "module",
  "main": "lib/index.js",
  "browser": "dist/rison2.js",
  "packageManager": "pnpm@8.6.11",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "default": "./lib/index.cjs"
    },
    "./lib/escaped": {
      "import": "./lib/escaped.js",
      "require": "./lib/escaped.cjs",
      "default": "./lib/escaped.cjs"
    }
  },
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.js .",
    "test": "jest",
    "clean": "rimraf lib dist",
    "build": "npm run bundle && npm run minify",
    "bundle": "rollup -c",
    "minify": "terser -c -m -o dist/rison2.min.js -- dist/rison2.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kou64yama/rison2.git"
  },
  "keywords": [
    "rison"
  ],
  "author": "Yamada Koji <kou64yama@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kou64yama/rison2/issues"
  },
  "homepage": "https://github.com/kou64yama/rison2#readme",
  "browserslist": [
    "last 2 versions",
    "> 0.2%",
    "not dead"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.2",
    "terser": "^5.19.2",
    "typescript": "^5.1.6"
  }
}
